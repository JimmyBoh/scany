{
  "name": "scany",
  "version": "0.3.0",
  "description": "Easily grab recent video data from user, channel, and playlist URLs!",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "gulp clean",
    "rebuild": "gulp build",
    "watch": "gulp watch",
    "test": "gulp test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmyBoh/scany.git"
  },
  "author": "Jim Buck <jim@jimmyboh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JimmyBoh/scany/issues"
  },
  "homepage": "https://github.com/JimmyBoh/scany#readme",
  "devDependencies": {
    "@types/cheerio": "^0.17.31",
    "@types/node": "^6.0.46",
    "ava": "^0.17.0",
    "coveralls": "^2.11.14",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.5.2",
    "nyc": "^10.0.0",
    "typescript": "^2.1.4",
    "yargs": "^6.3.0"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "feedparser": "^1.1.5",
    "got": "^6.6.3",
    "lodash.get": "^4.4.2",
    "url-pattern": "^1.0.2"
  },
  "ava": {
    "files": [
      "./dist/**/*.spec.js"
    ],
    "source": [
      "./dist/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.spec.js",
      "dist/debug.js"
    ]
  }
}
