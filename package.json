{
  "name": "scany",
  "version": "0.9.1",
  "description": "Easily grab video data from user, channel, playlist, and video URLs!",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist/** ./coverage",
    "build": "tsc",
    "watch": "concurrently npm:watch:*",
    "watch:build": "tsc --watch",
    "watch:test": "ava --watch",
    "test": "tsc && nyc --color -a ava -v",
    "debug": "tsc && node ./dist/debug",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmyBoh/scany.git"
  },
  "author": "Jim Buck <jim@jimmyboh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JimmyBoh/scany/issues"
  },
  "homepage": "https://github.com/JimmyBoh/scany#readme",
  "devDependencies": {
    "@types/cheerio": "^0.22.8",
    "@types/debug": "0.0.30",
    "@types/node": "^10.7.1",
    "@types/puppeteer": "^1.6.0",
    "ava": "^0.25.0",
    "concurrently": "^3.6.1",
    "coveralls": "^3.0.2",
    "nyc": "^12.0.2",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "feedparser": "^2.2.9",
    "got": "^9.0.0",
    "lodash.get": "^4.4.2",
    "pully-core": "^0.5.2",
    "puppeteer": "^1.7.0",
    "scrape-it": "^5.0.5",
    "url-pattern": "^1.0.2"
  },
  "ava": {
    "files": [
      "./dist/**/*.spec.js"
    ],
    "source": [
      "./dist/**/*.js"
    ]
  },
  "nyc": {
    "timeout": "2m",
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.spec.js",
      "dist/debug.js"
    ]
  }
}
